{
	"manifest_version": 3,
	"name": "OGP Checker",
	"version": "1.0.0",
	"description": "Check the object that is displayed when the URL is shared on SNS.",
	"permissions": [
		"activeTab",
		"scripting"
	],
	"host_permissions": [
		"*://*/*"
	],
	"devtools_page": "devtools.html",
	"background": {
		"service_worker": "background.js"
	}
}